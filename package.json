  },
  "homepage": "https://github.com/ccalilung/project3#readme",
  "dependencies": {
    "bcrypt": "^3.0.2",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "mongoose": "^5.3.4",
    "react": "^16.5.2",
    "request": "^2.88.0",
    "request-promise": "^4.2.2"
  }
}
