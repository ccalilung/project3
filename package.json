{
  "name": "project3",
  "version": "1.0.0",
  "description": "This application aims to give users the ability to research the nutritional value of popular products on supermarket shelves.",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'foodapp/*'\" \"npm run foodapp\"",
    "foodapp": "cd foodapp && npm run start",
    "heroku-postbuild": "cd foodapp && npm install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ccalilung/project3.git"
  },
